<script>
  function todaysdate() { var checkbox = document.getElementById('todayis'); if
  (checkbox.checked != false) document.getElementById('todayis').value =
  getDate(); if (checkbox.checked != true)
  document.getElementById('todayis').value = ""; } function getDate() { var date
  = new Date(); var day = date.getDate(); var month = date.getMonth() + 1; var
  year = date.getFullYear() - 2000; if (month < 10) month = "0" + month; if (day
  < 10) day = "0" + day; var today = year + "" + month + "" + day; return today;
  }
</script>